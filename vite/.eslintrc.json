{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-props-no-spreading": [<enabled>, {
            "html": "ignore" | "enforce",
            "custom": "ignore" | "enforce",
            "explicitSpread": "ignore" | "enforce",
            "exceptions": [<string>]
        }]
    }
}
